    <script>
        function show() {
            var x = document.getElementById("hidden");
            if (x.style.display === "block") {
                x.style.display = "none";
            } else {
                x.style.display = "block";
            }

            if (x.style.display === "block") {
                document.getElementById("btn").innerHTML = "Show Less &uarr;";
            }
        }
    </script>
